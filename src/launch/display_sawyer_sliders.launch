<?xml version="1.0"?>
<launch>
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find Teleoperacion-VisionArtificial)/src/urdf/sawyer.urdf.xacro" />
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
    <arg name="config_file" value="$(find Teleoperacion-VisionArtificial)/src/config/sawyer.rviz"/>
    <node name="rviz" pkg="rviz" type="rviz" respawn="false" output="screen" args="-d $(arg config_file)"/>
</launch>